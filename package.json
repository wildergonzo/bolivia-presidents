{
  "name": "bolivia-presidents",
  "version": "1.1.0",
  "description": "Get random Bolivian presidents.",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 70 --functions 80 --lines 100",
    "test": "istanbul cover _mocha -- -R spec test/*.test.js --compilers js:babel-register",
    "prebuild": "rm -rf dist",
    "build": "npm-run-all --parallel build:main build:umd.min",
    "build:main": "babel --copy-files --out-dir dist src",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:umd.min": "webpack --output-filename index.umd.min.js -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wildergonzo/bolivia-presidents.git"
  },
  "keywords": [
    "random",
    "bolivia",
    "presidents"
  ],
  "author": "Wilder Gonzales <wilder.gonzales@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wildergonzo/bolivia-presidents/issues"
  },
  "homepage": "https://github.com/wildergonzo/bolivia-presidents#readme",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.0.0",
    "ghooks": "^2.0.0",
    "install": "^0.8.7",
    "istanbul": "^1.0.0-alpha",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "npm": "^4.3.0",
    "npm-run-all": "^4.0.2",
    "webpack": "^2.2.1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test && npm run check-coverage"
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
